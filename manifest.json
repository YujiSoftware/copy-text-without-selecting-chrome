{
  "name": "Copy Text without Selecting",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Copy text without selecting. Easy and fast copy tool.",
  
  "permissions" : [
    "tabs", 
    "http://*/*", 
    "https://*/*", 
    "contextMenus"
  ],
  
  "icons" : {
    "48" : "icon.png"
  },
  
  "background" : {
    "scripts" : ["background.js"]
  }
}
